{
  "name": "multi-metal-token-backend",
  "version": "1.0.0",
  "description": "India's First Multi-Metal Token App - Backend API for Gold, Silver, Platinum, and Stablecoin trading",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "setup": "npm install && npm run setup:db",
    "setup:db": "node scripts/setup-database.js",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed-data.js",
    "build": "echo 'No build step required'",
    "deploy:dev": "npm run migrate && npm start",
    "deploy:prod": "NODE_ENV=production npm start",
    "docker:build": "docker build -t multimetal-token-backend .",
    "docker:run": "docker run -p 3001:3001 multimetal-token-backend"
  },
  "keywords": [
    "blockchain",
    "rwa",
    "tokenization",
    "gold",
    "silver",
    "platinum",
    "stablecoin",
    "trading",
    "crypto",
    "india",
    "mcx",
    "lppm",
    "sebi",
    "rbi"
  ],
  "author": "MiniMax Agent",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "ws": "^8.14.2",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "joi": "^17.9.2",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "mongodb": "^5.7.0",
    "redis": "^4.6.8",
    "fabric-network": "^2.2.19",
    "fabric-ca-client": "^2.2.19",
    "mongoose": "^7.5.0",
    "pg": "^8.11.3",
    "knex": "^2.5.1",
